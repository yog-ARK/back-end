<iframe
  src="https://f.top4top.io/m_3047q7zp11.mp3"
  height="0"
  width="0"
  allow="autoplay"
  ;
  encrypt-media
  style="display: none"
>
</iframe>

<audio autoplay>
  <source src="assets/Express Cafe.mp3" type="audio/mp3" />
</audio>
<header class="header">
  <nav>
    <div class="nav__bar">
      <div class="logo">
        <a href="#"><img src="assets/logo.png" alt="logo" /></a>
      </div>
      <div class="nav__menu__btn" id="menu-btn">
        <i class="ri-menu-line"></i>
      </div>
    </div>
    <ul class="nav__links" id="nav-links">
      <li><a href="/">Home</a></li>
      <li><a href="/about">About</a></li>
      <li><a href="/room">Rooms</a></li>
      <li><a href="/service">Services</a></li>
      <li><a href="/explore">Explore</a></li>
      <li><a href="/contact">Contact</a></li>
    </ul>
    <a href="/login" class="btn nav__btn">Login</a>
  </nav>
  <div class="section__container header__container" id="home">
    <p>Luxorious - Unique - Cozy</p>
    <h1><br />TAKE YOUR <span>REST</span>.</h1>
  </div>
</header>

<section class="section__container booking__container">
  <form action="/" class="booking__form">
    <div class="input__group">
      <span><i class="ri-calendar-2-fill"></i></span>
      <div>
        <label for="check-in">CHECK-IN</label>
        <input type="text" placeholder="6/5/2024" />
      </div>
    </div>
    <div class="input__group">
      <span><i class="ri-calendar-2-fill"></i></span>
      <div>
        <label for="check-out">CHECK-OUT</label>
        <input type="text" placeholder="7/5/2024" />
      </div>
    </div>
    <div class="input__group">
      <span><i class="ri-user-fill"></i></span>
      <div>
        <label for="guest"><%= username %></label>
        <input type="text" placeholder="User" />
      </div>
    </div>
    <div class="input__group input__btn">
      <button class="btn">CHECK OUT</button>
    </div>
  </form>
</section>
